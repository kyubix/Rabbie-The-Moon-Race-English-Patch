    <graphicData>
      <texPath>Things/Resource/Moonsilk</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
			<MaxHitPoints>200</MaxHitPoints>
      <MarketValue>12</MarketValue>
      <Mass>0.028</Mass>
      <Flammability>0.4</Flammability>
			<DeteriorationRate>1.0</DeteriorationRate>
      <StuffPower_Armor_Sharp>2.20</StuffPower_Armor_Sharp>
      <StuffPower_Armor_Blunt>0.72</StuffPower_Armor_Blunt>
