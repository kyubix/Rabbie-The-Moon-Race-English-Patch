<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	
  <RecipeDef>
    <defName>Make_Moonthread</defName>
    <label>make Moonthread</label>
    <description>The moon's synthetic fabric is made using fibers, organic materials and silver.</description>
    <jobString>Making moonthread.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Tailor</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>1800</workAmount>
    <allowMixingIngredients>true</allowMixingIngredients>
    <targetCountAdjustment>50</targetCountAdjustment>
    <recipeUsers>
      <li>RB_MoonTailoringBench</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Cloth</li>
          </thingDefs>
        </filter>
        <count>45</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Silver</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
      <li>
        <filter>
          <categories>
            <li>Leathers</li>
          </categories>
          <disallowedThingDefs>
            <li>Leather_Thrumbo</li>
          </disallowedThingDefs>
        </filter>
        <count>45</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Leathers</li>
      </categories>
      <disallowedThingDefs>
        <li>Leather_Patch</li>
        <li>Leather_Human</li>
      </disallowedThingDefs>
    </fixedIngredientFilter>
    <products>
      <Moonthread>50</Moonthread>
    </products>
    <researchPrerequisite>Moonthread</researchPrerequisite>
    <workSkill>Crafting</workSkill>
  </RecipeDef>

  
